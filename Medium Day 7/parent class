
class Parent {
    void parentMethod() {
        System.out.println("This is parent class");
    }
}

class Child extends Parent {
    void childMethod() {
        System.out.println("This is child class");
    }
}
public class Main {
    public static void main(String[] args) {
        Parent parent = new Parent();
        parent.parentMethod();
        Child child = new Child();
        child.childMethod();
        child.parentMethod();
    }
}
